extends layout

block layout-content
    div.container-fluid.userView
        div.row.justify-content-center
            h1 Howdy, #{userProfile.nickname}!
        div.row.justify-content-center
            h1 User Profile
        div.row.justify-content-center
            p=JSON.stringify(userProfile, null, 2)
        div.row.justify-content-center
            a(href="/logout") Log Out
